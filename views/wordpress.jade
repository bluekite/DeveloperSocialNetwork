extends layout-demo
include mixin-menu

block stylesheets
	link(rel="stylesheet",href="/stylesheets/graph.css")

block sidebar
	+menuOfActive('wordpress')

block content
	section.choice-section(style="margin:0 auto",id="choice-section")
		div(style="padding:10px")
			a.btn.btn-differShow.active(style="",href="#choice-section",id="wordpress") 关系网络演化
			a.btn.btn-differShow(style="",href="#table-section",id="trac") 分离关系网络
		div.choice-section-content
			table.choice-section-table
				tr(style="margin:10px auto")
					p(style="padding:5px 5px;") @ 通过选择项目的开发版本和开发者网络以来类型，来查看开发者社交网络不同版本的迭代，演化关系网络图。
					p(style="padding:5px 5px;") @ 如果进行版本的多重选择，则生成项目演化关系网络图，如果多种版本比较，则是项目不同关系网络的合成图。
				tr(style="margin:10px auto")
					td
						lable(style="padding:0 10px;")= "项目版本选择"
					td
						div.btn-group(data-toggle="buttons")
							label.btn.version.btn-choice 15
								input(type="checkbox") 
							label.btn.version.btn-choice 20
								input(type="checkbox") 
							label.btn.version.btn-choice 21
								input(type="checkbox")
							label.btn.version.btn-choice 22
								input(type="checkbox")
							label.btn.version.btn-choice 23
								input(type="checkbox")
							label.btn.version.btn-choice 25
								input(type="checkbox")
							label.btn.version.btn-choice 26
								input(type="checkbox")
							label.btn.version.btn-choice 27
								input(type="checkbox")
							label.btn.version.btn-choice 28
								input(type="checkbox") 
							label.btn.version.btn-choice 29
								input(type="checkbox") 
							label.btn.version.btn-choice 30
								input(type="checkbox") 
							label.btn.version.btn-choice 31
								input(type="checkbox") 
							label.btn.version.btn-choice 32
								input(type="checkbox") 
							label.btn.version.btn-choice 33
								input(type="checkbox") 
							label.btn.version.btn-choice 34
								input(type="checkbox") 
							label.btn.version.btn-choice 35
								input(type="checkbox") 
							label.btn.version.btn-choice 36
								input(type="checkbox") 
				tr(style="margin:20px auto")
					td
						lable(style="padding:0 10px;")= "开发者依赖关系选择"
					td
						div.btn-group(data-toggle="buttons")
							label.btn.developer.btn-choice developer_comment
								input(type="checkbox") 
							label.btn.developer.btn-choice developer_commit
								input(type="checkbox") 
							label.btn.developer.btn-choice developer_work
								input(type="checkbox") 
	section.evolution-section
		div.row(style="margin:10px auto;max-width:888px")
			ul.nav.nav-tabs(id="evolution-tab")
				li.active
					a(href="#graph-tab",data-toggle="tab") 开发者社交网络关系图
				li
					a(href="#parallel-tab",data-toggle="tab") 开发者网络中心度分析图
			div.tab-content
				div.tab-pane.active(style="margin:10px",id="graph-tab")		
					p(style="padding:5px 5px;text-align:left;color:#428bca;font-size:25px") 开发者社交网络关系图
					div.graph-section-content(id="graph")
				div.tab-pane(style="margin:10px",id="parallel-tab")
					p(style="padding:5px 5px;text-align:left;color:#428bca;font-size:25px") 开发者网络中心度分析图
					div.graph-section-content(id="parallel")
	//- section.graph-secion
	//- 	p(style="padding:5px 5px;text-align:left;color:#428bca;font-size:25px") 开发者社交网络关系图
	//- 	div.graph-section-content(id="graph")
	//- section.parallel-secion
	//- 	p(style="padding:5px 5px;text-align:left;color:#428bca;font-size:25px") 开发者网络中心度分析图
	//- 	div.graph-section-content(id="parallel")
	section.table-section(id="table-section")
		div(style="padding:10px")
			a.btn.btn-differShow(style="",href="#choice-section",id="wordpress") 关系网络演化
			a.btn.btn-differShow.active(style="",href="#table-section",id="trac") 分离关系网络			
		div.table-section-content
			table
				tr(style="margin:10px auto")
					td
						lable(style="padding:0 10px;")= "选择开发版本"
					td
						select(id="version-selector",style="min-height:50px;min-width:100px")
							option= "15"
							option= "20"
							option= "21"
							option= "22"
							option= "23"
							option= "25"
							option= "26"
							option= "27"
							option= "28"
							option= "29"
							option= "30"
							option= "31"
							option= "32"
							option= "33"
							option= "34"
							option= "35"
							option= "36"
	section.detailed-graph-section(id="detailed-graph-section")
		div.row(style="margin:10px auto;max-width:888px")
			ul.nav.nav-tabs(id="graph-tab")
				li.active
					a(href="#TT-graph",data-toggle="tab") TT
				li
					a(href="#developer-graph",data-toggle="tab") developer
				li
					a(href="#file-graph",data-toggle="tab") file
			div.tab-content
				div.tab-pane.active(style="margin:10px",id="TT-graph")
					div.col-md-12(id="TT-logic",style="border:1px solid #000000;margin:10px 0px")
						lable= "TT-logic-network" 
					div.col-md-12(id="TT-syntax",style="border:1px solid #000000;margin:10px 0px")
						lable= "TT-syntax-network" 
					div.col-md-12(id="TT-work",style="border:1px solid #000000;margin:10px 0px") 
						lable= "TT-work-network" 
				div.tab-pane(style="margin:10px",id="developer-graph")
					div.col-md-12(id="developer-comment",style="border:1px solid #000000;margin:10px 0px")
						lable= "developer-comment-network" 
					div.col-md-12(id="developer-commit",style="border:1px solid #000000;margin:10px 0px")
						lable= "developer-commit-network" 
					div.col-md-12(id="developer-work",style="border:1px solid #000000;margin:10px 0px") 
						lable= "developer-work-network"   
		div(style="margin:0 auto")   
			table.table(id="TT-analysis",style="display:block")
block javascripts 		      	
	script(src="/javascripts/d3.v3.min.js")
	script(src="/javascripts/demo-wordpress-graph.js")
