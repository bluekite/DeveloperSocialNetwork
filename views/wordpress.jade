doctype html
html
	head
		meta(charset="utf-8")
		title= title
		link(rel="stylesheet",href="/stylesheets/graph.css")
		link(rel="stylesheet",href="/stylesheets/bootstrap.min.css")
	body
		section.intro-section
			h1(style="text-align:center;color:#428bca") 开发者社交网络分析系统
		section.title-section
			h1(style="text-align:center;color:#428bca") 实验项目选择 
			br
			a.btn.btn-lg.btn-wordpress.active(style="",href="/demo/wordpress",id="wordpress") Wordpress
			a.btn.btn-lg.btn-trac(style="",href="/demo/wordpress",id="trac") Trac
		section.choice-section
			div(style="padding:10px;margin:0 auto")
				a.btn.btn-differShow.active(style="",href="/demo/wordpress",id="wordpress") 关系网络演化
				a.btn.btn-differShow(style="",href="/demo/wordpress",id="trac") 分离关系网络
			div.choice-section-content
				table.choice-section-table
					tr(style="margin:10px auto")
						p(style="padding:5px 5px;color: #ffffff;") @ 通过选择项目的开发版本和开发者网络以来类型，来查看开发者社交网络不同版本的迭代，演化关系网络图。
						p(style="padding:5px 5px;color: #ffffff;") @ 如果进行版本的多重选择，则生成项目演化关系网络图，如果多种版本比较，则是项目不同关系网络的合成图。
					tr(style="margin:10px auto")
						td
							lable(style="padding:0 10px;")= "项目版本选择"
						td
							div.btn-group(data-toggle="buttons")
								label.btn.version.btn-choice(style="border:1px solid #fff") 15
									input(type="radio") 
								label.btn.version.btn-choice(style="border:1px solid #fff") 20
									input(type="radio") 
								label.btn.version.btn-choice(style="border:1px solid #fff") 21
									input(type="radio")
								label.btn.version.btn-choice(style="border:1px solid #fff") 22
									input(type="radio")
								label.btn.version.btn-choice(style="border:1px solid #fff") 23
									input(type="radio")
								label.btn.version.btn-choice(style="border:1px solid #fff") 25
									input(type="radio")
								label.btn.version.btn-choice(style="border:1px solid #fff") 26
									input(type="radio")
								label.btn.version.btn-choice(style="border:1px solid #fff") 27
									input(type="radio")
								label.btn.version.btn-choice(style="border:1px solid #fff") 28
									input(type="radio") 
								label.btn.version.btn-choice(style="border:1px solid #fff") 29
									input(type="radio") 
								label.btn.version.btn-choice(style="border:1px solid #fff") 30
									input(type="radio") 
								label.btn.version.btn-choice(style="border:1px solid #fff") 31
									input(type="radio") 
								label.btn.version.btn-choice(style="border:1px solid #fff") 32
									input(type="radio") 
								label.btn.version.btn-choice(style="border:1px solid #fff") 33
									input(type="radio") 
								label.btn.version.btn-choice(style="border:1px solid #fff") 34
									input(type="radio") 
								label.btn.version.btn-choice(style="border:1px solid #fff") 35
									input(type="radio") 
								label.btn.version.btn-choice(style="border:1px solid #fff") 36
									input(type="radio") 
					//- tr
					//- 	td
					//- 		lable(style="padding:10px 10px")= "TT 网络多重选择"
					//- 	td
					//- 		div.btn-group(data-toggle="buttons")
					//- 			label.btn.TT.btn-choice(style="border:1px solid #fff") TT_logic
					//- 				input(type="checkbox") 
					//- 			label.btn.TT.btn-choice(style="border:1px solid #fff") TT_syntax
					//- 				input(type="checkbox") 
					//- 			label.btn.TT.btn-choice(style="border:1px solid #fff") TT_work
					//- 				input(type="checkbox") 
					tr(style="margin:20px auto")
						td
							lable(style="padding:0 10px;")= "开发者社交网络多重选择"
						td
							div.btn-group(data-toggle="buttons")
								label.btn.developer.btn-choice(style="border:1px solid #fff") developer_comment
									input(type="checkbox") 
								label.btn.developer.btn-choice(style="border:1px solid #fff") developer_commit
									input(type="checkbox") 
								label.btn.developer.btn-choice(style="border:1px solid #fff") developer_work
									input(type="checkbox") 
		section.graph-secion(style="min-height:500px;border-bottom:2px solid #428bca")
			p(style="padding:5px 5px;text-align:center;color:#428bca;font-size:25px") 开发者社交网络关系图
			div.graph-section-content(id="graph")
		section.parallel-secion(style="border-top:2px solid #428bca")
			p(style="padding:5px 5px;text-align:center;color:#428bca;font-size:25px") 开发者社交网络粒度分析平行表
			div.graph-section-content(id="parallel")
		section.table-section
			div.table-section-content
				table
					tr(style="margin:10px auto")
						td
							lable(style="padding:0 10px;")= "选择开发版本"
						td
							select(id="version-selector",style="min-height:50px;min-width:100px")
								option= "15"
								option= "20"
								option= "21"
								option= "22"
								option= "23"
								option= "25"
								option= "26"
								option= "27"
								option= "28"
								option= "29"
								option= "30"
								option= "31"
								option= "32"
								option= "33"
								option= "34"
								option= "35"
								option= "36"
				table.table(id="TT-analysis",style="display:block")
		section.detailed-graph-section(id="detailed-graph-section")
			div.row(style="margin:10px auto;max-width:888px")
				ul.nav.nav-tabs(id="graph-tab")
					li.active
						a(href="#TT-graph",data-toggle="tab") TT
					li
						a(href="#developer-graph",data-toggle="tab") developer
					li
						a(href="#file-graph",data-toggle="tab") file
				div.tab-content
					div.tab-pane.active(style="margin:10px",id="TT-graph")
						div.col-md-12(id="TT-logic",style="border:1px solid #000000;margin:10px 0px")
							lable= "TT-logic-network" 
						div.col-md-12(id="TT-syntax",style="border:1px solid #000000;margin:10px 0px")
							lable= "TT-syntax-network" 
						div.col-md-12(id="TT-work",style="border:1px solid #000000;margin:10px 0px") 
							lable= "TT-work-network" 
					div.tab-pane(style="margin:10px",id="developer-graph")
						div.col-md-12(id="developer-comment",style="border:1px solid #000000;margin:10px 0px")
							lable= "developer-comment-network" 
						div.col-md-12(id="developer-commit",style="border:1px solid #000000;margin:10px 0px")
							lable= "developer-commit-network" 
						div.col-md-12(id="developer-work",style="border:1px solid #000000;margin:10px 0px") 
							lable= "developer-work-network"        		
		script(src="/javascripts/jquery-1.11.0.min.js")
		script(src="/javascripts/bootstrap.min.js")        	
		script(src="/javascripts/d3.v3.min.js")
		script(src="/javascripts/demo-wordpress-graph.js")
